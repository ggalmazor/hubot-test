{
  "name": "hubot-test",
  "version": "1.0.9",
  "description": "Helper for testing hubot scripts",
  "keywords": [],
  "author": "Guillermo Gutierrez Almazor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggalmazor/hubot-test.git"
  },
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "linter": "eslint .",
    "linter-fix": "eslint --fix .",
    "format-fix": "prettier -w '*.{js,json}' '{src,tests}/**/*.js'",
    "format-check": "prettier --check '*.{js,json}' '{src,tests}/**/*.js'",
    "docs-build": "jsdoc src/index.js -c jsdoc.conf.json -t node_modules/ink-docstrap/template -R README.md",
    "docs-dev": "http-server out & watch 'npm run docs-build' src"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/index.js"
  ],
  "dependencies": {
    "hubot": "^11.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/jest": "^29.5.13",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "http-server": "^14.1.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "prettier": "^3.3.3",
    "taffydb": "^2.7.3",
    "uuid": "^10.0.0"
  }
}
